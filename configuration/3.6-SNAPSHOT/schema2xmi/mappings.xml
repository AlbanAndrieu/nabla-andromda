<!-- This is used to map SQL Types to model datatypes for Schema2XMI -->
<mappings name="DatatypeMappings">
    <mapping>
        <from>DECIMAL</from>
        <to>datatype::Decimal</to>
    </mapping>
    <mapping>
        <from>NUMERIC</from>
        <to>datatype::Float</to>
    </mapping>
    <mapping>
       <from>REAL</from>
       <to>datatype::Float</to>
    </mapping>
    <mapping>
        <from>INTEGER</from>
        <to>datatype::Integer</to>
    </mapping>
    <mapping>
       <from>SMALLINT</from>
       <to>datatype::Short</to>
    </mapping>
    <mapping>
        <from>TINYINT</from>
        <to>datatype::Byte</to>
    </mapping>
    <mapping>
        <from>NUMERIC(19,0)</from>
        <to>datatype::Long</to>
    </mapping>
    <mapping>
       <from>BIGINT</from>
       <to>datatype::Long</to>
    </mapping>
    <mapping>
        <from>FLOAT</from>
        <to>datatype::Float</to>
    </mapping>
    <mapping>
        <from>BOOLEAN</from>
        <to>datatype::Boolean</to>
    </mapping>
    <mapping>
       <from>BIT</from>
       <to>datatype::Boolean</to>
    </mapping>
    <mapping>
        <from>VARCHAR</from>
        <to>datatype::String</to>
    </mapping>
    <mapping>
        <from>LONGVARCHAR</from>
        <to>datatype::String</to>
    </mapping>
    <mapping>
        <from>CHAR</from>
        <to>datatype::Char</to>
    </mapping>
<!--
    <mapping>
       <from>CHAR</from>
       <to>datatype::Character</to>
    </mapping>
 -->
    <mapping>
        <from>TIMESTAMP</from>
        <to>datatype::Timestamp</to>
    </mapping>
    <mapping>
        <from>VARBINARY</from>
        <to>datatype::Timestamp</to>
    </mapping>
    <mapping>
        <from>DATETIME</from>
        <to>datatype::Timestamp</to>
    </mapping>
   <mapping>
        <from>DATE</from>
        <to>datatype::Date</to>
   </mapping>
    <!--
   <mapping>
      <from>DATETIME</from>
      <to>datatype::DateTime</to>
   </mapping>
    -->
   <mapping>
        <from>DOUBLE</from>
        <to>datatype::Float</to>
    </mapping>
    <mapping>
        <from>BINARY</from>
        <to>datatype::Byte[]</to>
    </mapping>
    <mapping>
        <from>BIT</from>
        <to>datatype::Byte</to>
    </mapping>
    <mapping>
       <from>LONGVARBINARY</from>
       <to>datatype::Blob</to>
    </mapping>
    <mapping>
        <from>TEXT</from>
        <to>datatype::Clob</to>
    </mapping>
    <mapping>
        <from>IMAGE</from>
        <to>datatype::Blob</to>
    </mapping>
    <mapping>
       <from>UNIQUEIDENTIFIER</from>
       <to>datatype::Guid</to>
    </mapping>
</mappings>
