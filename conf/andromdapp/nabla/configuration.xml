<?xml version='1.0' encoding='utf-8'?>
<andromdapp overwrite="true">
    <properties>
        <property name="andromdappType">j2ee</property>
        <property name="author">Alban Andrieu</property>
        <property name="modelType">uml2</property>
        <property name="applicationParentDirectory">$${project.basedir}</property>
        <property name="applicationId">nabla</property>
        <property name="applicationName">AndroMDA Nabla Sample</property>
        <property name="applicationVersion">$${project.version}</property>
        <property name="applicationPackage">${application.package}</property>
        <property name="applicationType">ear</property>
        <property name="transactionPersistenceType">spring</property>
        <property name="workflow">no</property>
        <property name="implementationLanguage">java</property>
        <property name="web">yes</property>
        <property name="webType">struts</property>
        <property name="webservice">yes</property>
        <property name="soapStack">axis</property>
        <property name="axisVersion">2</property>
        <property name="model">jar:file:${project.basedir}/src/main/uml/${uml.type}/nabla.xml.zip!/nabla.xml</property>
        <property name="configurationUri">${project.basedir}/src/main/config/andromda.xml</property>
        <property name="databaseType">hypersonic</property>
        <property name="distributionManagementRepositoryUrl">$${project.distributionManagement.repository.url}</property>
        <property name="distributionManagementRepositoryId">$${project.distributionManagement.repository.id}</property>
        <property name="distributionManagementSnapshotRepositoryUrl">$${project.distributionManagement.snapshotRepository.url}</property>
        <property name="distributionManagementSnapshotRepositoryId">$${project.distributionManagement.snapshotRepository.id}</property>
    </properties>
</andromdapp>